{
  "expo": {
    "name": "ILocker",
    "slug": "ILocker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.jpeg",
    "scheme": "ilocker",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ilocker.secure",
      "infoPlist": {
        "NSFaceIDUsageDescription": "ILocker uses Face ID to securely unlock your encrypted files",
        "NSCameraUsageDescription": "ILocker needs camera access to capture and encrypt photos",
        "NSPhotoLibraryUsageDescription": "ILocker needs access to your photo library to encrypt and secure your images",
        "NSPhotoLibraryAddUsageDescription": "ILocker needs permission to save decrypted files to your photo library",
        "UIBackgroundModes": [
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.ilocker.secure",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "ACCESS_MEDIA_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "allowBackup": false,
      "adaptiveIcon": {
        "backgroundColor": "#0f0f0f",
        "foregroundImage": "./assets/images/app-icon.jpeg",
        "backgroundImage": "./assets/images/app-icon.jpeg",
        "monochromeImage": "./assets/images/app-icon.jpeg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f0f0f",
          "dark": {
            "backgroundColor": "#0f0f0f"
          }
        }
      ],
      [
        "expo-secure-store"
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app-icon.jpeg",
          "color": "#4a90e2",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "ILocker Operations"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "777b6d83-09c7-4785-b890-29586970a63a"
      }
    }
  }
}
